<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <ApplicationIcon />
    <Win32Resource />
    <OutputType>Library</OutputType>
    <StartupObject />
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>TransbankDotNetSDK</PackageId>
    <Description>This is the .Net Standar SDK for Transbank Integration.</Description>
    <PackageLicenseUrl>https://raw.githubusercontent.com/continuum/transbank-onepay-sdk-dotNet/master/LICENSE?token=AAdOLqu-Hi6w2goUNU6LI8E3XSQPWMLUks5bZIR9wA%3D%3D</PackageLicenseUrl>
    <PackageProjectUrl>http://www.transbankdevelopers.cl/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/continuum/transbank-onepay-sdk-dotNet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageTags>payments, cl, chile, transbank</PackageTags>
    <PackageIconUrl>tbk_logo.icon</PackageIconUrl>
    <VersionPrefix>1.0.0</VersionPrefix>
    <VersionSuffix>$(VersionSuffix)</VersionSuffix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
